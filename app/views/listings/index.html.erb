<div class="rs-home-image">
  <div class="container">
    <%= render(partial: "wallpaper_overlay")%>
  </div>
</div>
<div class="container mt-4">
  <h5> Featured </h5>
  <hr>
  <div class="row mt-4">
    <% if @listings %>
      <% @listings.each do |listing| %>
        <div class="col-md-3 col-sm-12 mt-3">
          <%= render(partial: "listing_card", locals: {listing: listing}) %>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
<%= render(partial: "partials/footer") %>